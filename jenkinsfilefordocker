node{
stage ('scm checkout') {
git 'https://github.com/uttamrao/maven-project.git'
}
stage ('package stage') {
sh label: '', script: 'mvn clean package '

}
stage ('docker image build') {
sh 'docker build -t abhoyar9/prakash-app:1.0.0 .'
}

stage ('Push Docker image to DockerHub') {
withCredentials([string(credentialsId: 'dockerhubaccount', variable: 'dockerhub')]) {
sh "docker login -u abhoyar9 -p ${dockerhub}"
}
sh 'docker push abhoyar9/amitbhoyardocker'
}
}


